package ejercicio08;

public class CuentaCorriente {

	private double saldo;
	private String nombre;
	
	
		public CuentaCorriente() {
			
		this.saldo=0.0;
		this.nombre=nombre;
	
		
	}
		//Constructor vacÃ­o 
		public CuentaCorriente(){
			
		}
		public CuentaCorriente(double saldo, String nombre) {
			this.saldo=saldo;
			this.nombre=nombre;
		}
		
		public double retirar(double retirar) {
			
				
				if(retirar>this.saldo) {
					
					
					
				}else {
					this.saldo=this.saldo-retirar;
					
				}
				return this.saldo;
				
			
			
		}
		
		public double calcularDolares() {
			
			double dolares=1.16;
			
			double cambioDolares;
			
			cambioDolares=this.saldo*dolares;
			
			return cambioDolares;
		}
	
	
	
}

package ejemplopoliforfismo;

public class Circulo extends Figura {
	
	
	private double radio;
	
	
	public Circulo(){
		
	}

	public Circulo(String nombre, String color) {
		super(nombre, color);
	}

	public double getRadio() {
		return radio;
	}

	public void setRadio(double radio) {
		this.radio = radio;
	}
	
	
	
	
	
	
	
	

}
